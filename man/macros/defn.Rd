\newcommand{\old}{Logical. If old is \code{TRUE}, the simulation is implemented by R code; if old is \code{FALSE}, it is implemented by C code. Only for comparison of speed. Default option is \code{FALSE}.}
\newcommand{\pr}{Logical. If pr is \code{TRUE}, the function simulates data on target present trials; if pr is \code{FALSE}, it simulates data on target absent trials.}
\newcommand{\k}{A natural number representing the total number of stimuli in the display (set size).}
\newcommand{\esterrorpar}{A vector of length 5. Estimates of the accuracy-related parameters (\eqn{\alpha}, \eqn{\beta}, \eqn{a_1}, \eqn{a_2}, \eqn{b})}
\newcommand{\N}{A natural number representing the number of simulation runs.}
\newcommand{\par}{A vector of length 3 or 4, equals (miat, mst, Tres) if  non-decision time is assumed to be the same for no and yes responses (as in \link{WM}, \link{WMdiffresp} and \link{WMdiffrespweight}) and (\code{miat}, \code{mst}, \code{Tresn}, Tresy) otherwise (as in \code{WMdiffrespshift} and \code{WMdiffrespshiftweight}).}
\newcommand{\c}{A natural number representing the number of parallel servers of the system.}
\newcommand{\seed}{The random seed used in the simulation.}
\newcommand{\empRT}{A vector of empirical response times collected under given target presence and set size condition.}
